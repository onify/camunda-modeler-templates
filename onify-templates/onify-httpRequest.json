[
    {
        "name": "Onify | httpRequest | GET example",
        "id": "onify.httpRequest.get_example",
        "appliesTo": [
            "bpmn:ServiceTask"
        ],
        "description": "GET information from external API.",
        "version": 1,
        "metadata": {
            "tags": [
                "Onify",
                "Onify.httpRequest"
            ]
        },
        "entriesVisible": true,
        "properties": [
            {
                "label": "Result variable",
                "value": "getExampleResult",
